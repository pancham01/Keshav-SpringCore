<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean definitions here -->


	<bean name="add" class="com.my.spring.model.Address">
		<property name="city" value="Noida" />
		<property name="state" value="UP" />
	</bean>
	<bean name="emp1" class="com.my.spring.model.Employee">
		<property name="id" value="1011" />
		<property name="name" value="Vikash" />
		<property name="gender" value="male" />
		<property name="address" ref="add" />

		<property name="myList">
			<list>
				<value>1321</value>
				<value>1321</value>
				<value>21</value>
				<value>321</value>
				<value>121</value>
				<value>341</value>
				<value>221</value>
			</list>
		</property>
		<property name="setOfContacts">
			<set>
				<value>1321</value>
				<value>1321</value>
				<value>21</value>
				<value>321</value>
				<value>121</value>
				<value>341</value>
				<value>221</value>
			</set>
		</property>
		<property name="map">
			<map>
				<entry key="1" value="One" />
				<entry key="2" value="TWO" />
				<entry key="3" value="THREE" />
				<entry key="4" value="FOUR" />
				<entry key="1" value="ONE" />
			</map>
		</property>
	</bean>


	<!-- <bean name="emp2" class="com.my.spring.model.Employee"> <constructor-arg 
		name="id" value="2311" /> <constructor-arg name="name" value="Kubal" /> <constructor-arg 
		name="gender" value="male" /> <constructor-arg name="address" ref="add" /> 
		</bean> -->


</beans>